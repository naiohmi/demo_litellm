# Azure OpenAI GPT-4o model
AZURE_API_BASE=https://your-azure-openai-resource-name.openai.azure.com/
AZURE_OPENAI_API_KEY=your-gpt4o-api-key

# Azure OpenAI Embedding model (e.g. ada-002)
AZURE_API_KEY_TEXT_EMBEDDING_ADA_002=your-embedding-api-key

# LiteLLM Master Key (used for authentication, e.g. in Authorization headers)
LITELLM_MASTER_KEY=sk-1234567890
UI_USERNAME=litellm_user        # username to sign in on UI
UI_PASSWORD=litellm_pass        # password to sign in on UI

# Optional: Qdrant host and port (if not using default localhost:6333)
QDRANT_API_BASE=http://qdrant:6333

# Optional: Langfuse configuration
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=your-langfuse-host

# PostgreSQL (for LiteLLM or Langfuse)
POSTGRES_USER=litellm_user
POSTGRES_PASSWORD=litellm_pass
POSTGRES_DB=litellm_db
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# LiteLLM-compatible full DB URL
DATABASE_URL=postgresql://litellm_user:litellm_pass@postgres:5432/litellm_db